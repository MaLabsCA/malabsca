<?xml version="1.0"?>
<odoo>
  <data>

    <!-- EDI product malabscad record type -->
    <record id="malabscad_record_type" model="edi.record.type">
      <field name="name">Product Ma Labs CAD</field>
      <field name="model_id" ref="model_edi_product_malabscad_record"/>
      <field name="sequence" eval="900"/>
    </record>

    <!-- EDI product malabscad document type -->
    <record id="malabscad_document_type" model="edi.document.type">
      <field name="name">Product Ma Labs CAD</field>
      <field name="model_id" ref="model_edi_product_malabscad_document"/>
      <field name="sequence" eval="900"/>
      <field name="sequence_id" ref="sequence_default"/>
      <field name="rec_type_ids"
	     eval="[(6, False, [ref('malabscad_record_type')])]"/>
    </record>

    <!-- EDI product tutorial XML-RPC gateway path -->
    <record id="tutorial_path_xmlrpc" model="edi.gateway.path">
      <field name="name">Product Tutorial</field>
      <field name="sequence" eval="900"/>
      <field name="gateway_id" ref="edi.gateway_xmlrpc"/>
      <field name="path">tutorial/product</field>
      <field name="doc_type_ids"
	     eval="[(6, False, [ref('malabscad_document_type')])]"/>
    </record>

  </data>
</odoo>
